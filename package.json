{
  "name": "graph",
  "version": "1.0.0",
  "description": "A graph visualization lib.",
  "main": "src/index.js",
  "scripts": {
    "test": "ava --ta --verbose| tap-spec",
    "start": "webpack --progress --colors --watch"
  },
  "keywords": [
    "graph"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": ["babel-register", "babel-polyfill"]
  },
  "author": "chengang",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.23.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "jsdoc": "^3.4.3",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "tap-spec": "^4.1.1",
    "webpack": "^1.12.2",
    "@ava/babel-preset-stage-4": "^1.0.0"
  },
  "dependencies": {
  }
}
